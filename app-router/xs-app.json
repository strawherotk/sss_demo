{
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/mds/logout",
    "logoutPage": "/"
  },
  "routes": [{
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "destination": "ui5_dest",
      "authenticationType": "none"
    }, {
      "source": "^/mdmbp-svc/(.*)$",
      "target": "/$1",
      "destination": "mdmbp-svc",
      "authenticationType": "xsuaa"
    }, {
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "mds-node-api"
    }]
}