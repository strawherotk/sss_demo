{
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/mds/logout",
    "logoutPage": "/"
  },
  "routes": [{
      "source": "^/(.*)$",
      "target": "$1",
      "destination": "mds-node-api"      
    }, {
        "source":"^(.*)",
        "target":"mdsui5/$1",
        "service":"html5-apps-repo-rt"    
    }, {
        "source": "^/resources/(.*)$",
        "target": "/resources/$1",
        "destination": "ui5_dest",
        "authenticationType": "none"
    }]
}